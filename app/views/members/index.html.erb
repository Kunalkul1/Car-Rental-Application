<p id="notice"><%= notice %></p>
<h1>Members</h1>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%=member.name%></td>
        <td><%=member.email%></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td><br/>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
