<p id="notice"><%= notice %></p>
<h1>Current bookings</h1>
<table class="table table-striped table-bordered">
   <thead>
 <tr>
     <th>Booking Id</th>
     <th>Car Id</th>
     <th>Renter Id</th>
     <th>Time start</th>
     <th>Time end</th>
     <th colspan="3">Actions</th>
   </tr>
 </thead>

   <tbody>
 <% @bookings.each do |booking| %>
         <tr>
           <td><%=booking.id%></td>
           <td><%=booking.bookable_id%></td>
           <td><%=booking.booker_id%></td>
           <td><%=booking.time_start%></td>
           <td><%=booking.time_end%></td>
           <td><%=link_to 'Edit',edit_booking_path(booking)%></td>
         </tr>
     <% end %>
 </tbody>
 </table>

<%= link_to 'Back',root_path, class: "btn btn-primary btn-md text-white" %>