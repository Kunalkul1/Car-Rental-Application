<%= form_for(booking) do |f| %>
   <% if booking.errors.any? %>
     <div id="error_explanation">
       <h2><%= pluralize(booking.errors.count, "error") %> prohibited this booking from being saved:</h2>

       <ul>
         <% booking.errors.full_messages.each do |message| %>
                 <li><%= message %></li>
             <% end %>
       </ul>
     </div>
   <% end %>

   <div class="field">
     Car ID
     <%= f.text_field :bookable_id %>
   </div>

   <div class="field">
     Booker ID
     <%= f.text_field :booker_id %>
   </div>

   <div class="field">
     <%= f.label :time_start %>
     <%= f.datetime_field :time_start %>
   </div>

   <div class="field">
     <%= f.label :time_end %>
     <%= f.datetime_field :time_end %>
   </div>

   <div class="actions">
     <%= f.submit %>
   </div>
<% end %>