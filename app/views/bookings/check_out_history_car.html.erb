<table class = "table table-striped table-bordered">
  <thead>
  <tr>
    <th>License plate</th>
    <th>Model</th>
    <th>Style</th>
    <th>Booked by</th>
    <th>Start Time</th>
    <th>End Time</th>
  </tr>
  </thead>

  <tbody>
  <tr>
  <% @books1.each do |t| %>
    <td><%= @car.license_plate %></td>
    <td><%= @car.model %></td>
    <td><%= @car.style %></td>
    <td><%= find_user_name(t) %></td>
    <td><%= t.time_start %></td>
    <td><%= t.time_end %></td>
  </tr>
  <%end%>
   </tbody>
</table>
<%= link_to 'Back',root_path, class: "btn btn-primary btn-md text-white" %>